/// Render a waypoint circle with icon and level badge
fn render_waypoint(
    level_num: u8,
    is_complete: bool,
    x: u32,
    y: u32
) -> ByteArray {
    let mut svg: ByteArray = "";

    // Open group with translation
    svg.append(@"<g transform=\"translate(");
    svg.append(@u32_to_bytearray(x));
    svg.append(@",");
    svg.append(@u32_to_bytearray(y));
    svg.append(@")\">");

    // Circle background with appropriate color
    svg.append(@"<g fill=\"");
    if is_complete {
        svg.append(@complete_icon_color());
    } else {
        svg.append(@incomplete_icon_color());
    }
    svg.append(@"\" opacity=\"0.4\">");
    svg.append(@"<svg x=\"-25\" y=\"-25\" width=\"50\" height=\"50\" viewBox=\"0 0 405 180\">");
    svg.append(@get_circle_svg());
    svg.append(@"</svg></g>");

    // Icon overlay (check or question mark)
    svg.append(@"<g fill=\"");
    if is_complete {
        svg.append(@complete_icon_color());
    } else {
        svg.append(@incomplete_icon_color());
    }
    svg.append(@"\">");

    if is_complete {
        svg.append(@"<svg x=\"-18\" y=\"-40\" width=\"50\" height=\"50\" viewBox=\"0 0 62 63\">");
        svg.append(@get_check_svg());
    } else {
        svg.append(@"<svg x=\"-25\" y=\"-40\" width=\"50\" height=\"50\" viewBox=\"0 0 319 325\">");
        svg.append(@get_question_mark_svg());
    }
    svg.append(@"</svg></g>");

    // Level number badge
    svg.append(@"<g transform=\"translate(20, -20)\">");
    svg.append(@"<circle r=\"10\" fill=\"");
    if is_complete {
        svg.append(@complete_badge_color());
    } else {
        svg.append(@incomplete_badge_color());
    }
    svg.append(@"\" stroke=\"#fff\" stroke-width=\"2\"/>");
    svg.append(@"<text text-anchor=\"middle\" y=\"3.5\" fill=\"#fff\" font-size=\"10\" font-weight=\"bold\">");
    svg.append(@u32_to_bytearray(level_num.into()));
    svg.append(@"</text></g>");

    // Close group
    svg.append(@"</g>");

    svg
}

/// Render a path line between two waypoints
fn render_path(x1: u32, y1: u32, x2: u32, y2: u32) -> ByteArray {
    let mut svg: ByteArray = "";

    svg.append(@"<line x1=\"");
    svg.append(@u32_to_bytearray(x1));
    svg.append(@"\" y1=\"");
    svg.append(@u32_to_bytearray(y1));
    svg.append(@"\" x2=\"");
    svg.append(@u32_to_bytearray(x2));
    svg.append(@"\" y2=\"");
    svg.append(@u32_to_bytearray(y2));
    svg.append(@"\" stroke=\"");
    svg.append(@path_color());
    svg.append(@"\" stroke-width=\"3\" stroke-dasharray=\"5,5\" opacity=\"0.7\"/>");

    svg
}

